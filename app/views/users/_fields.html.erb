<%= form_for(@user) do | u | %>
  <% render 'shared/error_messages', object: u.object %>
  
  <%= u.label :name %>
  <%= u.text_field :name, autofocus: true %>

  <br />
  <%= u.label :profile %>
  <%= u.text_field :profile %>

  <br />
  <%= u.label :species %>
  <%= u.text_field :species %>
  
  <br /> 
  <%= u.label :gender %>
  <%= u.text_field :sex %>

  <br />
  <%= u.submit 'Create' %>
<% end %>


 
